    <named-native-query name="Cooperacion.AnhoProyecto" 
                        result-class="sv.gob.mined.cooperacion.model.dto.AtributoValorDto">
        <query>
             <![CDATA[
             select distinct 
                anho as atributo, 
                anho as valor
            from proyecto_cooperacion 
            order by to_number(anho)
            ]]>
        </query>    
    </named-native-query>
