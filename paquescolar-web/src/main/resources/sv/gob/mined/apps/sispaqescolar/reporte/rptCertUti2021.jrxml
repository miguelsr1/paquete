		</parameter>
		<queryString>
			<![CDATA[select 
    niv.descripcion_nivel
    masculino, 
    femenimo 
from estadistica_censo est 
    inner join nivel_educativo niv on niv.id_nivel_educativo = est.id_nivel_educativo
WHERE
     est.id_nivel_educativo IN (22,3,4,5,6) and 
     id_proceso_adq = $P{pIdProceso} and 
     codigo_entidad = $P{pCodigoEntidad}
ORDER BY
     niv.orden2]]>
		</queryString>
